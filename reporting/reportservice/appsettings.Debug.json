{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "JQueryCdnLink": "https://cdn.boldreports.com/external/",
    "ViewerCdnLink": "https://cdn.boldreports.com/dev/service/scripts/",
    "IsAzureApplication": "false",
    "EnableVirtualization": "false",
    "DisablePageSplitting": "false",
    "ShapePath": "/ShapeData/",
    "ExtAssemblies": "BoldReports.Data.WebData;BoldReports.Data.PostgreSQL;BoldReports.Data.Csv;BoldReports.Data.Excel;BoldReports.Data.MySQL;BoldReports.Data.Oracle;BoldReports.CRI.Barcode;BoldReports.Data.SSAS;BoldReports.Data.ElasticSearch;BoldReports.CRI.PredictiveAnalytics;BoldReports.Data.Snowflake;BoldReports.CRI.Signature;BoldReports.Data.GoogleBigQuery;BoldReports.CRI.Shape;BoldReports.Data.MongoDB;BoldReports.CRI.Html;BoldReports.CRI.Pdf;BoldReports.Data.AWSSecretsManager;",
    "CustomFontsPath": "custom-fonts",
    "Log4Net": {
      "ConfigFilePath": "/logs/Azure/Development/Log4Net.config",
      "ExceptionLess": {
        "ApiKey": "",
        "ServerUrl": ""
      }
    },
    "RateLimit": {
      "Enabled": false,
      "Limit": 50,
      "ResetTimeSeconds": 180,
      "Actions": [ "createshareddataset", "testconnection", "databases", "embeddeddata", "createserverreport", "createreportpart", "createdraftreport", "uploadreportaction" ]
    }
  }
}
