@{
    var dashboardServerResourceUrl = _globalAppSettings.SystemSettings.CDNLink.TrimEnd('/');
}

<environment include="Development,CloudDevelopment">
    <link rel="stylesheet" asp-append-version="true" href="@dashboardServerResourceUrl/cdn/css/custom-ui.css" />
</environment>
<environment exclude="Development,CloudDevelopment">
    <link rel="stylesheet" asp-append-version="true" href="@dashboardServerResourceUrl/cdn/css/custom-ui.min.css" />
</environment>

<div id="dashboard-rendering" class="col-xs-12" style="display:none">
    <iframe id="dashboard-rendering-iframe" style="height:100%;border:none"></iframe>
</div>